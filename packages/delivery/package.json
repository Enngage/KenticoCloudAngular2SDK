{
  "name": "kentico-cloud-delivery",
  "version": "6.0.0-beta.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/Enngage/kentico-cloud-js/tree/master/packages/delivery"
  },
  "private": false,
  "author": "richard sustek",
  "license": "MIT",
  "description": "Official Kentico Cloud Delivery SDK",
  "keywords": [
    "Kentico",
    "Kentico API",
    "Kentico Javascript",
    "Kentico SDK",
    "Kentico Node",
    "Kentico Cloud",
    "Kentico Cloud SDK",
    "Kentico Cloud Typescript",
    "Kentico Cloud Javascript",
    "Kentico Cloud Delivery",
    "Kentico Cloud Delivery SDK",
    "Kentico Delivery",
    "Kentico Delivery SDK",
    "Kentico Delivery Node",
    "Kentico Node SDK",
    "Kentico Node.js SDK"
  ],
  "engines": {
    "node": ">= 8"
  },
  "main": "./_commonjs/index.js",
  "types": "./_commonjs/index.d.ts",
  "scripts": {
    "typedoc-local": "",
    "tsc-local": "./node_modules/.bin/tsc",
    "webpack-local": "./node_modules/.bin/webpack",
    "karma-local": "node --max_old_space_size=8192 ./node_modules/karma/bin/karma",
    "ts-node-local": "./node_modules/.bin/ts-node",
    "mocha-local": "node ./node_modules/mocha/bin/mocha",
    "ts-lint-local": "./node_modules/.bin/tslint",
    "ts-lint:fix": "npm run ts-lint:check -- --fix",
    "ts-lint:check": "npm run ts-lint-local -- --project ./tsconfig.json",
    "test:node": "npm run build:commonjs && npm run mocha-local -- \"test-node/**/*.js\"",
    "test:browser": "npm run karma-local start karma.conf.js",
    "test:travis": "npm run karma-local start karma-travis.conf.js",
    "test:all": "npm run test:node && npm run test:travis",
    "new-patch": "npm version patch && npm run set-sdk-version && npm run check-version && npm run generate:docs",
    "new-minor": "npm version minor && npm run set-sdk-version && npm run check-version && npm run generate:docs",
    "new-major": "npm version major && npm run set-sdk-version && npm run check-version&& npm run generate:docs",
    "prepublishOnly": "npm run ts-lint:check && npm run set-sdk-version && npm run generate:docs && npm run check-version && npm run build:all && npm run test:all",
    "set-sdk-version": "node ./misc/set-sdk-version.js",
    "check-version": "npm run ts-node-local ./misc/verify-sdk-version.ts",
    "webpack:dev": "npm run webpack-local -- --mode development --config webpack.config.js --progress --colors",
    "webpack:prod": "npm run webpack-local -- --mode production --config webpack.config.js --progress --colors",
    "webpack:browser": "npm run webpack-local -- --mode production --config webpack.browser.config.js --progress --colors",
    "publish:beta": "npm publish --tag=next",
    "build:umd": "npm run webpack:dev && npm run webpack:prod && npm run webpack:browser",
    "build:es2015": "npm run tsc-local -- -m es2015 --outDir _es2015 --p tsconfig.prod.json",
    "build:commonjs": "npm run tsc-local -- -m commonjs --outDir _commonjs --p tsconfig.prod.json",
    "build:all": "npm run build:umd && npm run build:es2015 && npm run build:commonjs",
    "generate:docs": "cd ../../ && npm run typedoc:local -- --options packages/delivery/typedoc.json packages/delivery/lib/",
    "bundle-stats": "npm run build:umd && webpack-bundle-analyzer _bundles/stats.json",
    "browser-bundle-stats": "npm run build:umd && webpack-bundle-analyzer _bundles/browser-stats.json"
  },
  "dependencies": {
    "kentico-cloud-core": "1.4.4",
    "parse5": "5.1.0",
    "reflect-metadata": "0.1.13"
  },
  "peerDependencies": {
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.12",
    "@types/node": "11.12.0",
    "@types/parse5": "5.0.0",
    "colors": "1.3.3",
    "core-js": "3.0.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-typescript": "4.0.0",
    "karma-typescript-es6-transform": "4.0.0",
    "lcov-parse": "1.0.0",
    "mocha": "6.0.2",
    "rxjs": "6.4.0",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "typescript": "3.3.4000",
    "url-parse": "1.4.4",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0"
  }
}
